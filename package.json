        "eslint-plugin-simple-import-sort": "^7.0.0",
        "eslint-plugin-sort-keys-fix": "^1.1.1",
        "husky": "^6.0.0",
        "lint-staged": "^11.0.0",
        "prettier": "^2.3.0",
        "prettier-eslint": "^12.0.0",
        "pwa-asset-generator": "^4.1.1",
        "react-refresh": "^0.10.0",
        "react-scripts": "^4.0.3"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.src/**/*": "prettier --write"
    }
}
